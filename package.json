{
  "name": "ng4-signalr",
  "version": "1.2.0",
  "description": "angular4 signalr library",
  "main": "bundles/ng4-signalr.umd.js",
  "module": "index.js",
  "scripts": {
    "build": "rimraf ./dist && ng build --configuration production",
    "test": "karma start",
    "pack-lib": "npm pack ./dist",
    "publish-lib": "npm publish ./dist",
    "compodoc": "compodoc -p tsconfig.json",
    "compodoc-serve": "compodoc -s",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "cover": "istanbul cover node_modules/jasmine/bin/jasmine.js"
  },
  "typings": "index.d.ts",
  "author": "Hannes Neukermans",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astalegale/ng4-signalr.git"
  },
  "bugs": {
    "url": "https://github.com/astalegale/ng4-signalr/issues"
  },
  "homepage": "https://github.com/astalegale/ng4-signalr",
  "keywords": [
    "signalR",
    "typescript",
    "angular 4"
  ],
  "license": "MIT",
  "peerDependencies": {
    "jquery": "3.6.0",
    "signalr": "2.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.11",
    "@angular-devkit/core": "^12.2.11",
    "@angular/common": "^12.2.11",
    "@angular/compiler": "^12.2.11",
    "@angular/compiler-cli": "^12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/platform-browser": "^12.2.11",
    "@angular/platform-browser-dynamic": "^12.2.11",
    "@angular/router": "^12.2.11",
    "@nodelib/fs.walk": "^1.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^7.10.14",
    "@types/webpack-sources": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codelyzer": "^6.0.0",
    "core-js": "2.4.1",
    "coveralls": "^2.13.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "is-core-module": "^2.7.0",
    "istanbul": "^0.4.5",
    "ng-packagr": "^12.2.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "ts-helpers": "1.1.2",
    "ts-loader": "2.0.0",
    "typescript": "~4.3.5",
    "webpack": "2.2.1",
    "zone.js": "~0.11.4"
  },
  "dependencies": {
    "@angular/cli": "^12.2.11",
    "tslib": "^2.0.0"
  }
}
